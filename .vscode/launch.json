{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Gallery Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/build_output",
      "console": "integratedTerminal",
      "preLaunchTask": "Build Gallery",
      "serverReadyAction": {
        "pattern": "Serving HTTP on .* port ([0-9]+)",
        "uriFormat": "http://localhost:%s",
        "action": "openExternally"
      }
    }
  ]
}
